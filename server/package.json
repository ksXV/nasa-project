    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "axios": "^1.1.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "mongoose": "^6.6.1",
    "morgan": "^1.10.0",
    "pm2": "^5.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
