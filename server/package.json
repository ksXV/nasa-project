  "description": "NASA Mission Control Api",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "nodemon -x node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/server.ts",
    "start": "node --es-module-specifier-resolution=node ./dist/server.js",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "cluster": "pm2 start dist/server.js -i max",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.19.3",
